{
  "name": "basexer",
  "version": "0.1.0",
  "description": "The Blockchain Address service",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "npm run gulp-dev",
    "start": "run-s gulp-dev dev:fox",
    "start-chrome": "run-s gulp-dev dev:chrome",
    "dev:fox":"env-cmd -e fox -r ./.config/.env-cmdrc web-ext run --browser-console --browser-console",
    "dev:chrome": "env-cmd -e fox -r ./.config/.env-cmdrc web-ext run -t chromium --browser-console",
    "gulp-dev":"gulp dev:extension --max_old_space_size=1024",
    "test": "echo \"Error: no test specified\" && exit 1",
    "gulp-view": "gulp --tasks"
  },
  "repository": {
    "type": "git",
    "url": "git@flash:BASChain/httpb-plugin.git"
  },
  "keywords": [
    "Extendsions",
    "Blockchain"
  ],
  "author": "bas-fronter",
  "license": "MIT",
  "devDependencies": {
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "env-cmd": "^10.0.1",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-json-editor": "^2.5.4",
    "gulp-livereload": "^4.0.2",
    "gulp-merge-json": "^1.3.1",
    "gulp-multi-process": "^1.3.1",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-rtlcss": "^1.4.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-util": "^3.0.8",
    "gulp-watch": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "npm-run-all": "^4.1.5",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0",
    "web-ext": "^3.2.1"
  }
}